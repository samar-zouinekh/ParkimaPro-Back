                                    },
                                    "plate_info": {
                                        "type": "array",
                                        "items": {
                                            "properties": {
                                                "type": {
                                                    "type": "string",
                                                    "example": "tn"
                                                },
                                                "background": {
                                                    "type": "string",
                                                    "example": "background"
                                                },
                                                "inputPartOne": {
                                                    "type": "string",
                                                    "example": "---"
                                                },
                                                "inputPartTwo": {
                                                    "type": "string",
                                                    "example": "----"
                                                }
                                            },
                                            "type": "object"
                                        },
                                        "default": [
                                            {
                                                "type": "tn",
                                                "background": "background",
                                                "inputPartOne": "---",
                                                "inputPartTwo": "----"
                                            }
                                        ]
                                    },
                                    "license_plate": {
                                        "type": "string"
                                    },
                                    "ticket_duration": {
                                        "type": "integer"
                                    },
                                    "phone_number": {
                                        "type": "integer"
