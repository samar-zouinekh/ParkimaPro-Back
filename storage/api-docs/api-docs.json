        "/api/prepayment": {
            "post": {
                "tags": [
                    "Payment"
                ],
                "summary": "post Payment",
                "operationId": "makePrePayment",
                "requestBody": {
                    "description": "Station to create",
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "parking_id",
                                    "plate_info",
